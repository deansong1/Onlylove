te>\r\n<script>\r\nimport titleBar from \"./common/Titlebar\";\r\nimport messageList from \"./common/MessageList\";\r\nimport tabIcon from \"./common/TabBarIcon\";\r\nimport tabbaricon from \"./json/tabbar.json\";\r\nexport default {\r\n    data(){\r\n        return {\r\n            // tabbaricon,\r\n            // focused:\"\",\r\n            // normalImage:[\r\n            //     require('../../assets/ic_weixin_normal.png'),\r\n            //     require('../../assets/ic_contacts_normal.png'),\r\n            //     require('../../assets/ic_find_normal.png'),\r\n            //     require('../../assets/ic_me_normal.png'),\r\n            //     ],\r\n            // selectImage:[\r\n            //     require('../../assets/ic_weixin_selected.png'),\r\n            //     require('../../assets/ic_contacts_selected.png'),\r\n            //     require('../../assets/ic_find_selected.png'),\r\n            //     require('../../assets/ic_me_selected.png'),\r\n            //     ],\r\n            active:\"message\",\r\n            msg:\"微信(2)\",\r\n            // rfImg:'../../../assets/.png',\r\n            //这个require有动态生成返回值\r\n            rsImg:require('../../assets/ic_add.png'),\r\n\r\n            active:\"message\",\r\n            currentIndex:[\r\n                {isSelect:true},\r\n                {isSelect:false},\r\n                {isSelect:false},\r\n                {isSelect:false},\r\n            ]\r\n        }\r\n    },\r\n    methods:{\r\n        changeState(idx){\r\n            for(var i=0;i<this.currentIndex.length;i++){\r\n        //2:获取用户点击下标\r\n        var ui = idx;\r\n        //  与当前数据元素下标比较\r\n        //3:如果两个值相等当前状态true\r\n        if(ui==i){\r\n          this.currentIndex[i].isSelect=true;\r\n        }else{\r\n         //4:其它元素状态false\r\n         this.currentIndex[i].isSelect=false;\r\n        }\r\n            }\r\n        },\r\n        search(){\r\n            console.log(\"搜索\");\r\n        },\r\n        add3(){\r\n             console.log(\"添加\");\r\n        },\r\n    },\r\n    components:{\r\n        tabIcon,\r\n        messageList,\r\n        titleBar,\r\n        // \"titleBar\":titleBar,\r\n        // titleBar:titleBar,\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n    /* 修改组件tabbar默认样式 */\r\n    .mint-tabbar>.mint-tab-item{\r\n        color:#999;\r\n        padding:20px 0;\r\n    }\r\n    /* 修改组件tabbar选中时的默认样式 */\r\n    .mint-tabbar>.mint-tab-item.is-selected{\r\n        color:#45c018;\r\n    }\r\n</style>"]}]}